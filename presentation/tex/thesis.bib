@article{Friedrich1997,
abstract = {Odors are thought to be represented by a distributed code across the glomerular modules in the olfactory bulb (OB). Here, we optically imaged presynaptic activity in glomerular modules of the zebrafish OB induced by a class of natural odorants (amino acids [AAs]) after labeling of primary afferents with a calcium-sensitive dye. AAs induce complex combinatorial patterns of active glomerular modules that are unique for different stimuli and concentrations. Quantitative analysis shows that defined molecular features of stimuli are correlated with activity in spatially confined groups of glomerular modules. These results provide direct evidence that identity and concentration of odorants are encoded by glomerular activity patterns and reveal a coarse chemotopic organization of the array of glomerular modules.},
author = {Friedrich, R W and Korsching, S I},
file = {:home/andrey/Documents/Mendeley/Friedrich, Korsching/Friedrich, Korsching - 1997.pdf:pdf},
