@article{Avila2008,
abstract = {The competitive sorption of 1,2-cis-dichloroethene (cis-DCE) and trichloroethene (TCE) was investigated by means of column experiments using a model porous mineral solid represented by silica gel. The experimental isotherms were obtained by employing a chromatographic method. The competitive sorption isotherms were modelled with the extended Freundlich and extended Langmuir isotherms, using the parameters from single-solute experiments. The breakthrough curves were modelled with the advection-dispersion transport equation coupled with the lumped pore diffusion model. The best results were obtained when the extended Freundlich isotherm was employed. The competitive sorption was revealed with the presence of an overshoot in the breakthrough curve of cis-DCE and a decrease in the degree of sorption of cis-DCE (20\%) and TCE (12\%). A linear dependency of the overshoot with an increase in the concentration of cis-DCE at a fixed concentration of TCE was observed, between 16\% and 20\%, and at least at concentrations <6 mg L(-1) in the liquid phase. The displaced molecules of cis-DCE by TCE were accumulated through the column causing its overshoot; thus short columns may hinder its observation. Thermodynamic analysis shows an exothermic adsorption process of -34 to -41 kJ mol(-1), which is enhanced by sorption in micropores. The Gibbs free energy is positive for cis-DCE in the multi-component case, due to its displacement by TCE.},
author = {Avila, Manuel Alejandro Salaices and Breiter, Roman},
doi = {10.1016/j.chemosphere.2008.04.046},
file = {:home/andrey/Documents/Mendeley/Avila, Breiter/Avila, Breiter - 2008.pdf:pdf},
issn = {0045-6535},
journal = {Chemosphere},
keywords = {Adsorption,Dichloroethylenes,Dichloroethylenes: chemistry,Minerals,Minerals: chemistry,Silicon Dioxide,Silicon Dioxide: chemistry,Trichloroethylene,Trichloroethylene: chemistry},
month = aug,
number = {11},
pages = {1807--15},
pmid = {18541287},
title = {{Competitive sorption of cis-DCE and TCE in silica gel as a model porous mineral solid.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18541287},
volume = {72},
year = {2008}
}
